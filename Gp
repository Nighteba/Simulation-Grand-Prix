#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <stdbool.h>

typedef struct voiture voiture;
struct voiture //structure pour une voiture
{
        int numero;
        bool abandon;
        bool pitStop;
        int tpsCourseEssais[3];
        long tpsCourseQualifs[3];
};



int main(int argc, char *argV[]){
        int i, n, tot = 0; // variables de travail
        n = 3; // les trois temps du coureur 
        
voiture course; 


        srand((unsigned) time(NULL));
                for(i=0; i<n; i++){
                course.tpsCourseEssais[i] = rand() % 50; //remplie le tableau "tpsCourseEssais" avec des valeurs alÃ©atoires
                printf("laps: %d\n", + i );

                printf("%d\n",course.tpsCourseEssais[i]);

        }
                for(i=0; i<n; i++){

                        tot += course.tpsCourseEssais[i]; //fait la somme des 3 temps de course 
                }
                printf("tpsTotal %d",tot);

        return(0);
}

